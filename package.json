{
  "name": "blockchain-web3-specialist-framework",
  "version": "0.1.0",
  "description": "Comprehensive framework for building blockchain Web3 AI specialist agents with MCP integration",
  "private": true,
  "type": "module",
  "scripts": {
    "validate:docs": "tsx scripts/validate-docs.ts",
    "validate:docs:verbose": "tsx scripts/validate-docs.ts --verbose",
    "validate:docs:fix": "tsx scripts/validate-docs.ts --fix",
    "precommit": "pnpm validate:docs",
    "prepare": "husky install || true"
